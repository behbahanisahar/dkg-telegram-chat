{
  "name": "dkg-telegram-chat",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "start-dev-server": "craco start",
    "build": "craco build",
    "test": "node ./bin/sp-rest-proxy.js test-interactive",
    "test-debug": "node ./bin/sp-rest-proxy.js test-non-interactive",
    "test-interactive": "craco test",
    "test-non-interactive": "cross-env CI=true craco --inspect test --no-cache --runInBand",
    "test-coverage": "node ./bin/sp-rest-proxy.js test-ci",
    "test-vscode": "craco test --ci --coverage --reporters=default",
    "test-ci": "craco test --ci --coverage --reporters=default --reporters=jest-junit && node ./bin/inlineCoverageReportCSS.js",
    "analyze": "source-map-explorer build/static/js/main*",
    "analyze-vendor": "source-map-explorer build/static/js/1*",
    "eject": "craco eject",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "pre-commit-prettier": "pretty-quick --staged",
    "eslint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "deploy": "env-cmd -f .env.staging run-s build deploy-build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@baristalabs/craco-raw-loader": "^1.2.0",
    "@craco/craco": "^5.6.2",
    "@jackwilsdon/craco-use-babelrc": "^1.0.0",
    "@navikt/craco-plugins": "^1.0.1",
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.9",
    "@types/react": "^16.9.49",
    "@types/react-count-to": "^0.12.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-router-hash-link": "^1.2.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.5.3",
    "cache": "^2.1.0",
    "clean": "^4.0.2",
    "craco-babel-loader": "^0.1.4",
    "craco-less": "^1.16.0",
    "debug": "^4.1.1",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "install": "^0.13.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.14.1",
    "react": "^16.13.0",
    "react-app-polyfill": "^1.0.6",
    "react-bootstrap": "^1.3.0",
    "react-context-devtool": "^1.0.1",
    "react-dom": "^16.13.0",
    "react-promise-tracker": "^2.0.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.0.0",
    "react-scripts": "^3.3.0",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "envify": "^4.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "fs-jetpack": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-localize-redux": "^3.5.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "sp-pnp-node": "^2.2.0",
    "terser": "^4.7.0",
    "terser-brunch": "^4.0.0",
    "ts-node": "^8.10.1",
    "uglifyify": "^5.0.2"
  }
}
